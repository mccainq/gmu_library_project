<div class="jumbotron">
<%= form_for(:session, url: login_path, class: "nav-form") do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, autofocus: true, class: "form-control" %><br />
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %><br />
  <%= f.submit "Log In", class: "btn btn-default" %>
<% end %>
<div class="container">
<span>New user? <%= link_to "Sign up now!", new_user_path, class: "btn btn-lg btn-success" %></span>
</div>
</div>