<header>My Reservations</header>
<ol id="reservations">
  <% @reservations.each do |reservation| %>
    <li>
      <span class="reservation"><%= reservation.book.title %></span>
      <p style="float: left"><%= button_to 'Return Book', book_reservation_path(:book_id => reservation.book.id,
       :id => reservation.id), :class => "btn btn-danger",
               method: :delete %></p>
    </li>
  <% end %>
</ol>